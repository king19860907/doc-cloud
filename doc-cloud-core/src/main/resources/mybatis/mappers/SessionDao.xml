<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.doc.cloud.base.dao.SessionDao">

    <insert id="save">
        insert dc_session (id,session) values (#{sessionId},#{session})
    </insert>

    <update id="updateSession">
        update dc_session set session = #{session} where id = #{sessionId}
    </update>

    <delete id="deleteSessionById">
        delete from dc_session where id = #{sessionId}
    </delete>

    <select id="getSessionById" resultType="java.lang.String">
        select session from dc_session where id = #{sessionId}
    </select>

</mapper>